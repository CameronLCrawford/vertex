routine factorial (n: [8]) -> [8] {
        if n = 0:8 {
                return 1:8.
        }
        if n = 1:8 {
                return 1:8.
        }
        return [8](n * !factorial(n - 1:8)).
}

routine entry () -> [0];
        result: [8].
{
        set result = !factorial(5:8).
        output result.
        return.
}
